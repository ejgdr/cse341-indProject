{
  "name": "@mongosh/editor",
  "version": "1.4.2",
  "description": "MongoDB Shell External Editor",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/mongosh.git"
  },
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "test": "mocha -r \"../../scripts/import-expansions.js\" --timeout 60000 -r ts-node/register \"./{src,lib}/**/*.spec.ts\"",
    "test-ci": "node ../../scripts/run-if-package-requested.js npm test",
    "lint": "eslint --report-unused-disable-directives \"./{src,test}/**/*.{js,ts,tsx}\"",
    "check": "npm run lint && depcheck --skip-missing=true",
    "compile-ts": "tsc -p tsconfig.json",
    "prepublish": "npm run compile-ts"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "mongosh": {
    "unitTestsOnly": true
  },
  "dependencies": {
    "@mongosh/js-multiline-to-singleline": "1.4.2",
    "@mongosh/service-provider-core": "1.4.2",
    "@mongosh/shell-api": "1.4.2",
    "@mongosh/shell-evaluator": "1.4.2",
    "@mongosh/types": "1.4.2",
    "js-beautify": "^1.14.0"
  },
  "devDependencies": {
    "nanobus": "^4.5.0"
  },
  "gitHead": "7daf54d237b923567f44725816de7f62f471df11"
}
